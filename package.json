{
  "name": "justanesta-site",
  "version": "1.0.0",
  "description": "Personal portfolio and blog for justanesta.com - Built with 11ty and Bulma",
  "scripts": {
    "start": "npx @11ty/eleventy --serve",
    "build": "npx @11ty/eleventy && npx pagefind --site _site",
