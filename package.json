    "new:project": "./scripts/new-project.sh",
    "new:clip": "./scripts/new-clip.sh",
    "optimize": "./scripts/optimize-images.sh",
    "optimize:fix": "./scripts/optimize-images.sh --fix"
  },
  "keywords": [
    "portfolio",
    "blog",
    "11ty",
    "eleventy",
    "bulma"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "pagefind": "^1.4.0"
  },
  "dependencies": {
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "bulma": "^1.0.0"
  }
